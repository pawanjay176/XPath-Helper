{
  "manifest_version": 2,
  "name": "XPath Helper",
  "version": "1.0.13",
  "description": "Extract, edit, and evaluate XPath queries with ease.",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["lib/typo/typo.js","content.js"]
    }
  ],
  "permissions": ["tabs", "identity", "identity.email"],
  "icons": {
    "32": "static/icon32.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png"
  },
  "web_accessible_resources": [
    "lib/typo/dictionaries/en_US/en_US.aff",
    "lib/typo/dictionaries/en_US/en_US.dic",
    "lib/typo/typo.js",
    "bar.css",
    "bar.html",
    "bar.js"
  ]
}
